config_version=5

[application]

config/name="itemfinity"
run/main_scene="res://scenes/titlescreen.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=30
boot_splash/bg_color=Color(0.29211, 0, 0.0279529, 1)
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://other/audio.tres"

[autoload]

Spawning="*res://addons/BulletUpHell/Spawning.tscn"
Global="*res://scripts/gobal.gd"
TransitionManager="*res://scripts/transition_manager.gd"

[display]

window/size/viewport_width=400
window/size/viewport_height=400
window/size/initial_position_type=4
window/size/extend_to_title=true
window/size/window_width_override=800
window/size/window_height_override=800
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/BulletUpHell/plugin.cfg")

